    "import pandas\n",
    "from sklearn.svm import SVC\n",
    "\n",
    "data = pandas.read_csv('svm-data.csv', header = None)\n",
    "X = data.drop([0], axis = 1)\n",
    "y = data[0]\n",
    "svm = SVC(C = 100000, random_state = 241)\n",
    "svm.fit(X, y)\n",
    "print(svm.support_ + 1)"
